@page "/scheduleMeeting"

<PageTitle>Schedule Meeting</PageTitle>

<h1>Select Date</h1>
<input type="text" name="date" id="@this.GetHashCode()" />

<p role="status">Current count: @currentCount</p>

<h1>Select Time</h1>
<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
@code {
    protected override async void OnAfterRenderAsync(bool firstRender)
    {
        if(firstRender)
            await jsRuntime.InvokeVoidAsync("renderjQueryComponents", this.GetHashCode());
        await base.OnAfterRenderAsync(firstRender);
    }
}